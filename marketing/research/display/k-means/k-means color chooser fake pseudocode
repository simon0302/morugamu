arraylist whichColor = [empty];

function distance (point, center)
*returns a number*



for (i = 0; i < points.length; i++) {

int mindistance = int_max;

    for (j = 0; j < 5; j++) {


        if (min >= distance (point.get(i), center.get(j)) {
        min = distance (point.get(i), center.get(j))

        whichColor.get(i) = j

        }
